	public static String updateProizvod(String nazivProizvoda, String verzijaProizvoda, Image photo) {
		if (nazivProizvoda == null) {
			return "Unesite ime studenta";
		}
		nazivProizvoda = nazivProizvoda.trim();
		if (nazivProizvoda.isEmpty()) {
			return "Unesite ime studenta";
		}

		if (verzijaProizvoda == null) {
			return "Unesite prezime studenta";
		}
		verzijaProizvoda = verzijaProizvoda.trim();
		if (verzijaProizvoda.isEmpty()) {
			return "Unesite verziju proizvoda";
		}

		return "Model uspešno ažuriran";
	}

}